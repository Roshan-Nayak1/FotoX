{
    "version": 2,
    "builds": [
        {
            "src": "routes/index.js", // Adjust the pattern based on your project structure
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/routes/index.js" // Adjust the path to match your project structure
        }
    ]
}